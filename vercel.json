{
    "functions": {
        "main.py": {
            "memory": 3008,
            "maxDuration": 30
        }
    },
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}
  